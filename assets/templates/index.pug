<!DOCTYPE html>
html(lang="es") 
head
	title Tienda
	include global/t_head
	link(rel="stylesheet", href="css/home.css")
	
body
	include global/header
	main
		.home
			.home-blockA
				#carouselExampleControls.carousel.slide(data-ride='carousel')
					.carousel-inner
						.carousel-item.active
							img.d-block.w-100(src='img/banner/img1.jpg', alt='First slide')
						.carousel-item
							img.d-block.w-100(src='img/banner/img2.jpg', alt='Second slide')
						.carousel-item
							img.d-block.w-100(src='img/banner/img3.jpg', alt='Third slide')
					a.carousel-control-prev(href='#carouselExampleControls', role='button', data-slide='prev')
						span.carousel-control-prev-icon(aria-hidden='true')
							span.sr-only Previous
					a.carousel-control-next(href='#carouselExampleControls', role='button', data-slide='next')
						span.carousel-control-next-icon(aria-hidden='true')
							span.sr-only Next

			.home-blockB
				.container
					section.d-flex.flex-wrap.blockB-list
						article
							img
							h3
							button Comprar	
	
	include global/footer
	include global/t_script
